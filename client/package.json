{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "prettier": {
    "printWidth": 125
  },
  "scripts": {
    "dev:currency-exchanger": "vite --config src/apps/currency-exchanger/vite.config.ts",
    "build:currency-exchanger": "tsc -p src/apps/currency-exchanger/tsconfig.json && vite build --config src/apps/currency-exchanger/vite.config.ts",
    "preview:currency-exchanger": "vite preview --config src/apps/currency-exchanger/vite.config.ts",
    "dev:search-recipes": "vite --config src/apps/search-recipes/vite.config.ts",
    "build:search-recipes": "tsc -p src/apps/search-recipes/tsconfig.json && vite build --config src/apps/search-recipes/vite.config.ts",
    "preview:search-recipes": "vite preview --config src/apps/search-recipes/vite.config.ts",
    "dev:todo-app": "vite --config src/apps/todo-app/vite.config.ts",
    "build:todo-app": "tsc -p src/apps/todo-app/tsconfig.json && vite build --config src/apps/todo-app/vite.config.ts",
    "preview:todo-app": "vite preview --config src/apps/todo-app/vite.config.ts",
    "dev:products-listing": "vite --config src/apps/products-listing/vite.config.js",
    "build:products-listing": "vite build --config src/apps/products-listing/vite.config.js",
    "preview:products-listing": "vite preview --config src/apps/products-listing/vite.config.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.11.0",
    "sass-embedded": "^1.79.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
